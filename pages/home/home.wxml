<!-- 1.插槽的使用 :单个插槽-->
<my-slot>
  <!-- 1.插入按钮 -->
  <button size="mini" type="primary">我是插入的按钮</button>
</my-slot>

<my-slot>
  <text>hello, 我是你爹</text>
</my-slot>
<view>--------------------</view>
