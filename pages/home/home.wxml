<!-- 注意事项: -->
  <!-- 1.使用插槽时,要给每个插槽起一个name属性 -->
  <!-- 2.给components.js文件添加一个options:{mutipleSlots: true} -->

<my-mslot>
  <text slot='slot2'>我是slot2</text>
</my-mslot>